	//팝업 닫기 (팝업 타입에 W(윈도우오픈), I(iframe팝업), L(레이어드팝업))
    $("div.pop_tit_close").click(function(){
//	     	alert(1);
    	//iframe 형태의 팝업일 경우
    	if ("${search.popType}" == "I") {
    		parent.closeLayerPop();
    	} else {
    		window.close();
    	}
    	
    });
		
		
	    
});

$(window).load(function() {
	
	
	loadDetail();
	
});
function loadDetail(){
	var scriptJson = opener.grid_sheet.GetSaveJson(0);
	var objId = "&objId=";
	
	for (var i=0; i<scriptJson.data.length; i++) {
		objId += scriptJson.data[i].objId;
		if(i<scriptJson.data.length-1) {
			objId += "|";
			
		}
	}
	
	$('div#ddlScriptDtl').load('<c:url value="/meta/ddltsf/ajaxgrid/ddltsfscript_pop_dtl.do"/>', objId, function(){
		
// 	alert("test1");
	});
// 	alert("test");
}


</script>
    
